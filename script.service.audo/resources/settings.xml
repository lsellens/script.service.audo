<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<settings>
   <!-- audo -->
    <category label="32000">
		<setting label="32001" type="lsep"/>
		<setting type="sep"/>
		<setting id="SABNZBD_USER" type="text" label="32002" default=""/>
		<setting id="SABNZBD_PWD" type="text" label="32003" default=""/>
		<setting id="SABNZBD_LAUNCH" type="bool" label="32004" default="true" enable="eq(1,false)"/>
		<setting id="NZBGET_LAUNCH" type="bool" label="32005" default="false" enable="eq(-1,false)"/>
		<setting id="SICKBEARD_LAUNCH" type="bool" label="32006" default="true"/>
		<setting id="COUCHPOTATO_LAUNCH" type="bool" label="32007" default="true"/>
		<setting id="HEADPHONES_LAUNCH" type="bool" label="32008" default="true"/>
		<setting label="32009" type="lsep"/>
		<setting type="sep"/>
		<setting id="SABNZBD_IP" type="text" label="32010" default="0.0.0.0"/>
		<setting label="32011" type="lsep" visible="[! System.Platform.Linux.RaspberryPi] + System.CanSuspend"/>
		<setting type="sep" visible="[! System.Platform.Linux.RaspberryPi] + System.CanSuspend"/>
		<setting id="SABNZBD_KEEP_AWAKE" type="bool" label="32012" default="false" enable="eq(-9,true)"
                         visible="[! System.Platform.Linux.RaspberryPi] + System.CanSuspend"/>
		<setting id="PERIODIC_WAKE" type="bool" label="32013" default="false"
                         visible="[! System.Platform.Linux.RaspberryPi] + System.CanSuspend"/>
		<setting id="WAKE_AT" type="enum" subsetting="true" enable="eq(-1,true)"
		         label="32014" default="01"
		         values="|01:00|03:00|05:00|07:00|09:00|11:00|13:00|15:00|17:00|19:00|21:00|23:00"
                         visible="[! System.Platform.Linux.RaspberryPi] + System.CanSuspend"/>
                <setting id="RESTART_ON_RESUME" type="bool" label="32015" default="true"
                         visible="[! System.Platform.Linux.RaspberryPi] + System.CanSuspend"/>
    </category>
    <category label="32100">
		<setting label="32100" type="lsep"/>
		<setting type="sep"/>
                <setting label="32101" type="action" action="RunScript($CWD/changlogs.py, script.module.audo-programs)"/>
                <setting label="32102" type="action" action="RunScript($CWD/changlogs.py, script.service.audo)"/>
                <setting label="32103" type="action" action="RunScript($CWD/changlogs.py, script.module.audo-dependencies)"/>
    </category>
</settings>
